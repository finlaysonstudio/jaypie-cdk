{
  "name": "@jaypie/cdk",
  "version": "1.0.0",
  "author": "Finlayson Studio",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "src/index.js",
  "scripts": {
    "build": "BABEL_ENV=commonjs babel src --out-dir dist/cjs",
    "format": "npm run format:package && npm run format:lint",
    "format:lint": "eslint --fix .",
    "format:package": "sort-package-json ./package.json",
    "init:deploy": "hygen jaypie workflow-npm",
    "lint": "eslint .",
    "new": "hygen jaypie vite",
    "test": "vitest",
    "test:spec:cfnOutput.function": "vitest run ./src/__tests__/cfnOutput.function.spec.js",
    "test:spec:index": "vitest run ./src/__tests__/index.spec.js",
    "test:spec:isValidHostname.function": "vitest run ./src/__tests__/isValidHostname.function.spec.js",
    "test:spec:isValidSubdomain.function": "vitest run ./src/__tests__/isValidSubdomain.function.spec.js",
    "test:spec:mergeDomain.function": "vitest run ./src/__tests__/mergeDomain.function.spec.js",
    "test:spec:projectTagger.function": "vitest run ./src/__tests__/projectTagger.function.spec.js"
  },
  "dependencies": {
    "@jaypie/core": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@jaypie/testkit": "^1.0.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vitest": "^0.4.0",
    "hygen": "^6.2.11",
    "jest-extended": "^4.0.2",
    "prettier": "^3.2.5",
    "sort-package-json": "^2.10.0",
    "vitest": "^1.4.0"
  }
}
